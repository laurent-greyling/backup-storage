@model Final.BackupTool.Mvc.Models.StatusModel

@{
    ViewBag.Title = "title";
}
<meta http-equiv="refresh" content="20">

<h3>@ViewData["operation"]</h3>

<table class="table-fill">
    <thead>
    <tr>
        <th class="text-left table-fill"></th>
        <th class="text-left table-fill">Operation Type</th>
        <th class="text-left table-fill">Copied</th>
        <th class="text-left table-fill">Skipped</th>
        <th class="text-left table-fill">Faulted</th>
        <th class="text-left table-fill">Finished In</th>
        <th class="text-left table-fill">Status</th>
    </tr>
    </thead>
    <tbody class="table-hover table-fill">
    <tr>
        <td class="text-left table-fill">Table Details</td>
        <td class="text-left table-fill">@ViewData["TableOperationType"]</td>
        <td class="text-left table-fill">@ViewData["TablesCopied"]</td>
        <td class="text-left table-fill">@ViewData["TablesSkipped"]</td>
        <td class="text-left table-fill">@ViewData["TablesFaulted"]</td>
        <td class="text-left table-fill">@ViewData["TablesFinishedIn"]</td>
        <td class="text-left table-fill">@ViewData["TableStatus"]</td>
    </tr>
    <tr>
        <td class="text-left table-fill">Blob Details</td>
        <td class="text-left table-fill">@ViewData["BlobsOperationType"]</td>
        <td class="text-left table-fill">@ViewData["BlobsCopied"]</td>
        <td class="text-left table-fill">@ViewData["BlobsSkipped"]</td>
        <td class="text-left table-fill">@ViewData["BlobsFaulted"]</td>
        <td class="text-left table-fill">@ViewData["BlobsFinishedIn"]</td>
        <td class="text-left table-fill">@ViewData["BlobStatus"]</td>
    </tr>
    </tbody>
</table>

<h3>@ViewData["operation"] Details</h3>

<table class="table-fill">
    <thead>
        <tr>
            <th class="text-left table-fill"></th>
            <th class="text-left table-fill">Source</th>
            <th class="text-left table-fill">Status</th>
            <th class="text-left table-fill">Extra Information</th>
        </tr>
    </thead>
    <tbody class="table-hover table-fill">
        <tr>
            <td class="text-left table-fill">@ViewData["Source"]</td>
            <td class="text-left table-fill">@ViewData["Status"]</td>
            <td class="text-left table-fill">@ViewData["ExtraInfo"]</td>
        </tr>
    </tbody>
</table>


